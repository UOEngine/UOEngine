		<EmbeddedResource Include="FNACompat\Graphics\Effect\StockEffects\FXB\BasicEffect.fxb">
			<LogicalName>Microsoft.Xna.Framework.Graphics.Effect.Resources.BasicEffect.fxb</LogicalName>
		</EmbeddedResource>
	</ItemGroup>-->
	
</Project>
