<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<AssemblyName>FNA</AssemblyName>
	<RootNamespace>Microsoft.Xna.Framework</RootNamespace>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Platforms>x64</Platforms>
	  <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
	  <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="FNA\**" />
    <Compile Remove="Overrides\**" />
    <EmbeddedResource Remove="FNA\**" />
    <EmbeddedResource Remove="Overrides\**" />
    <None Remove="FNA\**" />
    <None Remove="Overrides\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="FNA3D.cs" />
    <Compile Remove="FNA3D.UOEngine.cs" />
    <Compile Remove="FNAPlatform.cs" />
    <Compile Remove="FNAPlatform.UOEngine.cs" />
    <Compile Remove="UOEGameWindow.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="StbImageSharp" Version="2.30.15" />
    <PackageReference Include="System.IO.Hashing" Version="10.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\UOEngine.Runtime.Core\UOEngine.Runtime.Core.csproj" />
    <ProjectReference Include="..\UOEngine.Runtime.Platform\UOEngine.Runtime.Platform.csproj" />
    <ProjectReference Include="..\UOEngine.Runtime.Plugin\UOEngine.Runtime.Plugin.csproj" />
  </ItemGroup>

